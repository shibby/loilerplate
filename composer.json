{
    "name": "shibby/loilerplate",
    "description": "My boilerplate for Laravel projects. Yes, i ALMOST use these packages on every project.",
    "type": "library",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "license": "GPL",
    "authors": [
        {
            "name": "Guven Atbakan",
            "email": "guven@atbakan.com"
        }
    ],
    "require": {
        "php": ">=7.0",
        "kris/laravel-form-builder": "^1.7",
        "laravelcollective/html": "^5.4",
        "doctrine/dbal": "^2.5",
        "jenssegers/agent": "^2.3",
        "sofa/revisionable": "^5.4",
        "predis/predis": "^1.1"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.2",
        "barryvdh/laravel-debugbar": "^2.2"
    },
    "autoload": {
        "psr-4": {
            "Shibby\\Loilerplate\\": "src/"
        }
    }
}
